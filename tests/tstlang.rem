if version()<"03.00.08"
   errmsg %
   errmsg This file only works with Remind version 03.00.08 and later - aborting
   exit
endif

if !$RunOff || !$DontQueue || $DontTrigAts
   errmsg %
   errmsg Please run [filename()] with the -q and -r options, but%
   errmsg not the -a option.
   exit
endif

set $AddBlankLines 0
# Include a language file, if that's given as "-ii=/path"

if defined("i")
    do [i]
endif

REM MSG Language: %(LANGID)
# Set up a few useful definitions
fset show(x) iif(x == "!" || x == "?", "%%" + x + " yields: " + char(34) + "%" + x + char(34), "%%" + x + " yields: " + char(34) + "%" + x + char(34) + "% and %%*" + x + " yields: " + char(34) + "%*" + x + char(34) + "%")
set a trigger(today()+2) + " ++2"
set l language()
set tt now()+134
set tu now()-134
set ut now()+60
set uu now()-60
set vt now()+120
set vu now()-120
set wt now()-1
set wu now()+1
set xt now()-2
set xu now()+2

set d a + " at " + tt
